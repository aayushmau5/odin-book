{
  "name": "server",
  "version": "1.0.0",
  "description": "Odin-book backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon ./dist/index.js",
    "watch": "tsc -w",
    "test": "npm run prisma:reset && jest --runInBand",
    "build": "tsc",
    "ts:start": "ts-node ./src/index.ts",
    "prisma:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema ./src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema ./src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:reset": "dotenv -e .test.env -- prisma migrate reset --schema ./src/prisma/schema.prisma --force"
  },
  "author": "aayushmau5",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.26.0",
    "apollo-server-express": "^2.25.2",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "flat": "^5.0.2",
    "google-auth-library": "^7.2.0",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-query-complexity": "^0.8.1",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^2.26.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.6",
    "@types/flat": "^5.0.1",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^16.0.0",
    "dotenv-cli": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
